<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="demo.example.lovechat"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="15" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="demo.example.lovechat.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="demo.example.lovechat.MeetActivity"></activity>
        <activity android:name="demo.example.lovechat.ContactActivity"></activity>
        <activity android:name="demo.example.lovechat.InvitedActivity"></activity>
        <activity android:name="demo.example.lovechat.ProfileActivity"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>

</manifest>
